пер счет цел;

про главная() нч
    для счет := 1..100 нч
        если счет % 15 == 0 нч
            печать(«ФиззБазз\н»);
        иначе если счет % 3 = 0 нч
            печать(«Физз\н»);
        иначе если счет % 5 = 0 нч
            печать(«Базз\н»);
        иначе нч
            печать(счет, «\н»);
        кц
    кц
