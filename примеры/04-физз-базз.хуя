вкл прелюдия;

пер счет: цел;

про главная() нч
    // // СДЕЛАТЬ: цикл «для»
    // для счет := 1..100 нч
    счет := 0;
    пока счет мн 100 нч
        если       счет % 15 == 0 то печать(«ФиззБазз\н»);
        иначе если счет % 3 = 0   то печать(«Физз\н»);
        иначе если счет % 5 = 0   то печать(«Базз\н»);
        иначе то печать(счет, «\н»);
        счет := счет + 1;
    кц
кц
