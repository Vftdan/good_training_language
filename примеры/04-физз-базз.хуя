вкл прелюдия;

про главная() нч
    для счет := 1..100 нч
        если          счет ост 15 = 0 то печать(«ФиззБазз\н»);
        // СДЕЛАТЬ: подумать как сократить конструкцию «иначе то если»
        иначе то если счет ост 3 = 0  то печать(«Физз\н»);
        иначе то если счет ост 5 = 0  то печать(«Базз\н»);
        иначе то печать(счет, «\н»);
    кц
кц
