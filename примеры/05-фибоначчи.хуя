вкл прелюдия;

конст ЛИМИТ = 1000;

про главная() нч
    // // СДЕЛАТЬ: определение нескольких переменных на одной строке
    // пер а, б, в: цел;
    пер а: цел;
    пер б: цел;
    а := 0;
    б := 1;
    пока а мн ЛИМИТ нч
        печать(а, «\н»);
        пер в: цел;
        в := а + б;
        а := б;
        б := в;
    кц
